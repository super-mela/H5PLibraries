{
  "env": {
    "es2020": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", "eslint-config-ndla-h5p", "plugin:jsdoc/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["jsdoc"],
  "settings": {
    "jsdoc": {
      "preferredTypes": {
        "Function": "function"
      }
    }
  }
}
